{
    "name": "horde/test",
    "description": "Unit testing library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Test",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        },
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        }
    ],
    "version": "2.6.4",
    "time": "2017-11-11",
    "repositories": [
        {
            "type": "pear",
            "url": "https://pear.horde.org"
        }
    ],
    "require": {
        "php": "^5.3.3 || ^7",
        "pear-pear.horde.org/Horde_Support": "^2",
        "pear-pear.horde.org/Horde_Util": "^2"
    },
    "suggest": {
        "pear-pear.horde.org/Horde_Cli": "^2",
        "pear-pear.horde.org/Horde_Log": "^2",
        "ext-dom": "*",
        "ext-json": "*"
    },
    "replace": {
        "pear-pear.horde.org/Horde_Test": "2.*",
        "pear-horde/Horde_Test": "2.*"
    },
    "bin": [
        "bundle/vendor/phpunit/phpunit/phpunit"
    ],
    "autoload": {
        "psr-0": {
            "Horde_Test": "lib/"
        }
    }
}
